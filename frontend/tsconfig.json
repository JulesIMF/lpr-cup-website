{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "moduleResolution": "Node",
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "lib": [
        "ES2021.String",
        "dom"
      ]
    },
    "include": [
      "**/*.ts",
      "**/*.tsx",
      "**/*.html",
    ],
}
